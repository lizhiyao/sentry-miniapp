{
  "name": "sentry-miniapp",
  "version": "0.9.3",
  "description": "用于小程序平台的 Sentry SDK",
  "repository": "git://github.com/lizhiyao/sentry-miniapp.git",
  "homepage": "https://github.com/lizhiyao/sentry-miniapp",
  "miniprogram": "dist",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:min": "npm-run-all --parallel build:wx build:my build:tt build:dd",
    "build:wx": "webpack --config ./webpack.config.wx.js",
    "build:my": "webpack --config ./webpack.config.my.js",
    "build:tt": "webpack --config ./webpack.config.tt.js",
    "build:dd": "webpack --config ./webpack.config.dd.js",
    "watch": "webpack --watch --config ./webpack.config.wx.js",
    "build": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "version": "node ./scripts/versionbump.js src/version.ts"
  },
  "keywords": [
    "sentry",
    "weapp",
    "miniapp",
    "Sentry SDK",
    "Sentry 小程序 SDK",
    "小程序 Sentry SDK",
    "异常监控",
    "异常上报",
    "小程序异常监控",
    "微信小程序",
    "支付宝小程序",
    "钉钉小程序",
    "字节跳动小程序"
  ],
  "author": "dancerlzy@gmail.com",
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@sentry-internal/eslint-config-sdk": "^8.34.0",
    "@sentry-internal/typescript": "8.34.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.7.7",
    "eslint": "^9.13.0",
    "install": "^0.13.0",
    "miniprogram-api-typings": "^3.4.7",
    "npm": "^10.9.0",
    "npm-run-all": "^4.1.5",
    "replace-in-file": "^8.2.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.10.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@sentry/core": "8.34.0",
    "@sentry/types": "8.34.0",
    "@sentry/utils": "8.34.0",
    "tslib": "^2.8.0"
  }
}