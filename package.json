{
  "name": "sentry-miniapp",
  "version": "0.12.1",
  "description": "用于小程序/小游戏平台的 Sentry SDK",
  "repository": "git://github.com/junqing258/sentry-miniapp.git",
  "homepage": "https://github.com/junqing258/sentry-miniapp",
  "miniprogram": "dist",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "vite build --minify terser",
    "dev:weapp": "vite build --watch --outDir examples/weapp/vender",
    "prepack": "vite build",
    "lint": "tsc --noEmit",
    "version": "node ./scripts/versionbump.js src/version.ts"
  },
  "keywords": [
    "sentry",
    "weapp",
    "miniapp",
    "Sentry SDK",
    "Sentry 小程序 SDK",
    "小程序 Sentry SDK",
    "异常监控",
    "异常上报",
    "小程序异常监控",
    "微信小程序",
    "支付宝小程序",
    "钉钉小程序",
    "字节跳动小程序"
  ],
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@swc/core": "^1.15.3",
    "@types/node": "^12.7.1",
    "change-case": "^5.4.4",
    "install": "^0.13.0",
    "minimist": "^1.2.8",
    "miniprogram-api-typings": "^4.0.4",
    "replace-in-file": "^8.3.0",
    "terser": "^5.37.0",
    "typescript": "^5.7.2",
    "vite": "^7.2.0",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "@sentry/core": "8.55.0",
    "@sentry/types": "8.55.0",
    "@sentry/utils": "8.55.0"
  }
}
