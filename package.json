{
  "name": "sentry-miniapp",
  "version": "0.9.4",
  "description": "用于小程序平台的 Sentry SDK",
  "repository": "git://github.com/lizhiyao/sentry-miniapp.git",
  "homepage": "https://github.com/lizhiyao/sentry-miniapp",
  "miniprogram": "dist",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:dist": "npm-run-all --parallel build:wx build:my build:tt build:dd",
    "build:wx": "webpack --config ./webpack.config.wx.js",
    "build:my": "webpack --config ./webpack.config.my.js",
    "build:tt": "webpack --config ./webpack.config.tt.js",
    "build:dd": "webpack --config ./webpack.config.dd.js",
    "build:watch": "webpack --watch --config ./webpack.config.wx.js",
    "build": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "version": "node ./scripts/versionbump.js src/version.ts"
  },
  "keywords": [
    "sentry",
    "weapp",
    "miniapp",
    "Sentry SDK",
    "Sentry 小程序 SDK",
    "小程序 Sentry SDK",
    "异常监控",
    "异常上报",
    "小程序异常监控",
    "微信小程序",
    "支付宝小程序",
    "钉钉小程序",
    "字节跳动小程序"
  ],
  "author": "dancerlzy@gmail.com",
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "@sentry/typescript": "^5.3.0",
    "@types/node": "^12.7.1",
    "install": "^0.13.0",
    "miniprogram-api-typings": "^2.7.7-2",
    "npm": "^6.11.1",
    "npm-run-all": "^4.1.5",
    "replace-in-file": "^4.1.3",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@sentry/core": "5.15.4",
    "@sentry/types": "5.15.4",
    "@sentry/utils": "5.15.4",
    "tslib": "^1.10.0"
  }
}
